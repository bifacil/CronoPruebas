---
UniqueId: dhgTfyjQyi
Title: "Archivo - Todos los artículos de Crono Business Intelligence"
Url: blog/index.html
Layout: layouts/blog.cshtml
Id: 0

---

<h2 class="page-title">Últimos articulos</h2>

@{
  var posts=(from post in Site.Pages where !post.Date.IsMinDate() orderby post.Date descending select post).Take(8);
  foreach(var post in posts){
<div class="blog-post">
  <div class="row">
	<div class="col-md-6">
		@if(post.Image!=null){<div class="imgpost"><img class="img-responsive" src="@Root/blog/images/@post.Image"></div>
		<p class="blog-post-meta">
			<span><i class="fa fa-archive"></i>@post.ShortDate</span>
			<span><i class="fa fa-tags"></i>business, intelligence, quote</span>
		</p>}
	</div>
	<div class="col-md-6">
		<h3><a href="@Root/@post.Url">@post.Title</a></h3>
		<p>@post.Description</p>
		<a href="@Root/@post.Url" class="btn">Leer más</a> </div>
  </div>
</div>
}}

<div class="row">
	<div class="col-md-8">
		<a href="@Root/blog/archive.html" class="btn readmore">Ver artículos anteriores</a> 
	</div>
</div>